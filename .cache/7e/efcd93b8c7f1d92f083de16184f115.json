e":"searchArtist","generated":{"line":114,"column":22},"source":"components/App.js","original":{"line":46,"column":30}},{"generated":{"line":114,"column":34},"source":"components/App.js","original":{"line":46,"column":8}},{"generated":{"line":115,"column":0},"source":"components/App.js","original":{"line":46,"column":44}},{"generated":{"line":115,"column":8},"source":"components/App.js","original":{"line":46,"column":44}},{"generated":{"line":115,"column":17},"source":"components/App.js","original":{"line":46,"column":53}},{"generated":{"line":115,"column":19},"source":"components/App.js","original":{"line":46,"column":54}},{"generated":{"line":116,"column":0},"source":"components/App.js","original":{"line":46,"column":8}},{"generated":{"line":116,"column":18},"source":"components/App.js","original":{"line":41,"column":6}},{"generated":{"line":116,"column":19},"source":"components/App.js","original":{"line":40,"column":4}},{"generated":{"line":117,"column":0},"source":"components/App.js","original":{"line":49,"column":3}},{"name":"Component","generated":{"line":121,"column":2},"source":"components/App.js","original":{"line":5,"column":18}},{"name":"App","generated":{"line":123,"column":15},"source":"components/App.js","original":{"line":51,"column":15}}],"sources":{"components/App.js":"import React, { Component } from \"react\";\n\nconst API_ADDRESS = 'https://spotify-api-wrapper.appspot.com';\n\nclass App extends Component {\n  state = { artistQuery: '', artist: null, tracks: [] };\n\n  updateArtistQuery = event => {\n    this.setState({ artistQuery: event.target.value });\n    console.log(\"event.target.value\", event.target.value);\n  }\n\n  searchArtist = () => {\n    console.log('state', this.state);\n    fetch(`${API_ADDRESS}/artist/${artistQuery}`)\n      .then(response => response.json())\n      .then(json => {\n        if (json.artists.total > 0) {\n          const artist = json.artists.items[0];\n\n          this.setState({ artist });\n\n        }\n      })\n      .catch(error => alert(error.message));\n  }\n\n  handleKeyPress = event => {\n    if (event.key === 'Enter') {\n      this.searchArtist();\n\n      fetch(`${API_ADDRESS}/artist/${artist.id}/top-tracks`)\n        .then(response => response.json())\n        .then(json => this.setState({ tracks: json.tracks }))\n        .catch(error => alert(error.message));\n    }\n  }\n\n  render() {\n    return (\n      <div>\n        <h1>Music Master</h1>\n        <input onChange={this.updateArtistQuery}\n          onKeyPress={this.handleKeyPress}\n          placeholder=\"Type the artist's name\" />&nbsp;\n        <button onClick={this.searchArtist} className=\"btn btn-default\">Search</button>\n      </div>\n    )\n  }\n}\nexport default App;"},"lineCount":null}},"error":null,"hash":"9acfa5e8ee7cb3cbbb95e35e1068b4db","cacheData":{"env":{}}}